# Thanos AI Gateway Environment Variables
# Copy to /etc/thanos/thanos.env and uncomment the ones you need

# Anthropic API Key
#ANTHROPIC_API_KEY=sk-ant-xxx

# OpenAI API Key
#OPENAI_API_KEY=sk-xxx

# Google Gemini API Key
#GEMINI_API_KEY=xxx

# xAI API Key
#XAI_API_KEY=xxx

# Logging
#RUST_LOG=info
#THANOS_LOG_FORMAT=json

# Custom config path (default: /etc/thanos/config.toml)
#THANOS_CONFIG=/etc/thanos/config.toml
